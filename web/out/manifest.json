{
    "name": "StarShooter",
    "description": "A MuPDF Based PDF Reader",
    "icons": [
        {
            "src": "/images/icon-512.png",
            "type": "image/png",
            "sizes": "512x512"
        }
    ],
    "start_url": "/",
    "scope": "/",
    "display": "standalone",
    "file_handlers": [
        {
            "action": "/open-csv",
            "accept": {
                "application/pdf": [
                    ".pdf"
                ]
            },
            "icons": [
                {
                    "src": "/images/icon-512.png",
                    "type": "image/png",
                    "sizes": "512x512"
                }
            ],
            "launch_type": "multiple-clients"
        }
    ]
}